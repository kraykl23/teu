{
  "version": 2,
  "functions": {
    "api/fetch-telegram.js": {
      "memory": 256,
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/fetch-telegram",
      "dest": "/api/fetch-telegram.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
